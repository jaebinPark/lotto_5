{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": { "L5": "readonly", "toast": "readonly", "nav": "readonly" },
  "rules": {
    "no-unused-vars": ["warn", { "args": "none" }],
    "no-console": "warn"
  }
}
